pipeline{

    agent any

    stages{
        stage("git clone"){
            steps{
                git 'https://github.com/manikantak2762/myweb.git'

            }
        }
        stage("clean"){
            steps{
                sh "mvn clean"
            }
        }
        stage("Test"){
            steps{
                sh "mvn test"
            }
        }
    }
}