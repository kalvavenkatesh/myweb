pipeline{

    agent any

    stages{
        stage("Git clone"){
            git 'https://github.com/manikantak2762/myweb.git'
        }

    }
    stage("clean"){
        steps{
            sh "mvn clean"
        }
    }
    stage("Test"){
        steps{
            sh "mvn test"
        }
        
    }
    stage(""Build"){
        sh "mvn package"
        sh "mv target/.* target/myweb.war"
        
    }

}